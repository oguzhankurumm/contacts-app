{
  "expo": {
    "name": "contacts-app",
    "slug": "contacts-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/tempExpoIcons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/tempExpoIcons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/tempExpoIcons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/tempExpoIcons/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
